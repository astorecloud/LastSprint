pipeline {
    agent {label "app" } 
    
	 stages {
        
        stage ('GitHub Integration & Artifact clean') {
            steps {
                checkout()
                sh 'rm -rf /Glueproject@tmp'
                sh 'ls'
                sh 'rm -rf $HOME_DIR/artifact'
                sh 'rm -rf $HOME_DIR/Glueproject/artifact.tar.gz'
            }
        
		
		 stage ('GitHub Integration & Artifact clean') {
            steps {
                checkout()
                sh 'rm -rf /Glueproject@tmp'
                sh 'ls'
                sh 'rm -rf $HOME_DIR/artifact'
                sh 'rm -rf $HOME_DIR/Glueproject/artifact.tar.gz'
            }
        
    }